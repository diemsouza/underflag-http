{
  "name": "underflag-http",
  "version": "1.0.6",
  "description": "http provider for underflag",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "test": "jest --silent=false",
    "prepublish": "npm run test && npm run build",
    "example": "npx ts-node examples/http",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dieg0s0uza/underflag-http.git"
  },
  "keywords": [
    "feature flag",
    "feature toggle",
    "remote config",
    "http"
  ],
  "author": "Diego Souza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dieg0s0uza/underflag-http/issues"
  },
  "homepage": "https://github.com/dieg0s0uza/underflag-http#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ]
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.8",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2",
    "underflag": "^2.1.0"
  },
  "dependencies": {
    "axios": "^0.25.0"
  },
  "peerDependencies": {
    "underflag": "^2.1.0"
  }
}